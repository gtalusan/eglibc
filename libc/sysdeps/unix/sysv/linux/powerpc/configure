$as_echo_n "checking whether $CC $CFLAGS -mlong-double-128 uses IBM extended format... " >&6; }
if test "${libc_cv_mlong_double_128ibm+set}" = set; then
  $as_echo_n "(cached) " >&6
else
  save_CFLAGS="$CFLAGS"
